generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Order {
  id             String    @id @default(uuid())
  clientId       String
  statusHistory  Json      
  createdAt      DateTime  @default(now())
  updatedAt      DateTime? @updatedAt
}
